@ARTICLE{vanGenderen1997,
  author = {Van Genderen, M.J. and Böttger, A. and Mittemeijer, E.J.},
  title = {Formation of $\alpha^{\prime\prime}\mkern-1.2mu$ iron nitride in
	{F}e{N} martensite: Nitrogen vacancies, iron-atom displacements,
	and misfit-strain energy},
  journal = {Metallurgical and Materials Transactions A},
  year = {1997},
  volume = {28},
  pages = {63-77},
  number = {1},
  abstract = {The precipitation of ␣" iron nitride in FeN martensite (containing
	about 5.9 N/100 Fe) is studied as a function of tempering time successively
	at 333 and 373 K by means of X-ray diffractometry (XRD). On the basis
	of peak (ϭlattice parameter) shifts and intensity changes of main
	and superstructure reflections, it is concluded that, during tempering,
	the structure of the formed ␣" precipitates changes. At 333 K, initially
	stoichiometric Fe16N2 is formed and, with further tempering, structural
	vacancies are introduced, i.e., Fe16N2Ϫx is formed. At 373 K, the
	vacancies are partially filled. The change of the ␣" structure at
	333 K is caused by the change of the matrix from martensite to ferrite,
	i.e., the misfit between the ␣" and the matrix is initially relatively
	small and then increases. By allowing nitrogen-deficient ␣", the
	increase of the misfit can be diminished at the expense of some increase
	of the volume of ␣". Gibbs free energy calculations (at 333 K), in
	which differences in the misfit energy are decisive, indeed show
	that, in a martensite matrix, Fe16N2 is favored, whereas, in a ferrite
	matrix, Fe16N2Ϫx is favored. Further, in this article, methods are
	presented to calculate structure factors for nitrogen-deficient ␣"
	and to obtain, from measured (ϭstrained) lattice parameters, strain-free
	␣" lattice parameters from which the composition can be deduced.},
  comment = {cited in 2016MATSCIA1},
  doi = {10.1007/s11661-997-0083-9},
  file = {Article:references/1997_vanGenderen_Nitrogen_Martensite.pdf:PDF},
  issn = {1073-5623},
  language = {English},
  publisher = {Springer-Verlag},
  review = {Tempering at 333 K of an FeN martensite (with an in- terstitial content
	of 5.9N/100Fe) leads to precipitation of the ␣" transition nitride.
	In the very first stage of ␣" pre- cipitation, where the matrix is
	practically martensite, the precipitates consist of perfect Fe16N2.
	Upon prolonged tem- pering at this temperature, where the matrix
	becomes fer- ritic, the precipitates consist of nitrogen-deficient
	Fe16N2Ϫx, such that structural vacancies are introduced on the nitrogen
	sublattice. After tempering at 333 K for 110 hours, the (strained)
	c lattice parameter of ␣" is 0.621 nm (the nitro- gen content is
	X␣ " ϭ 10.75 and the displacements of the Fe atoms are ⌬z ϭ 0.039
	and ⌬x ϭ 0.0028). Analysis of Gibbs free energy changes shows, in
	the case of elastic accommodation of the volume misfit between the
	␣" particles and the matrix, that (1) in a martensite matrix, perfect
	␣" nitride is favored over nitrogen-deficient ␣" ni- tride and that
	(2) in a ferrite matrix, nitrogen-deficient ␣" nitride is favored
	over perfect ␣" nitride. For (1), the dif- ferences in the ordering
	energy are decisive, whereas, for (2), the differences in misfit
	energy are decisive. Subse- quent tempering at 373 K leads to filling
	of nitrogen va- cancies due to small energy contributions caused
	by periodic arrangement of the precipitates. The c lattice parameter
	of unstrained perfect ␣"-Fe16N2 is predicted to be 0.6338 nm; on
	this basis, the c lattice param- eter of perfect ␣"-Fe16N2 confined
	in a ferrite matrix is cal- culated to be 0.6287 nm. The experimental
	value is 0.6283 nm (the nitrogen content is X␣ " ϭ 12.45 and the
	displace- ments of the Fe atoms are ⌬z ϭ 0.047 and ⌬x ϭ 0.0034).
	The value for the nearest-neighbor Fe-N distance in the c direction
	is about 0.187 nm.}
}

@ARTICLE{vanGent1985,
  author = {Van Gent, A. and Van Doorn, F.C. and Mittemeijer, E.J.},
  title = {Crystallography and tempering behavior of iron-nitrogen martensite},
  journal = {Metallurgical Transactions A},
  year = {1985},
  volume = {16},
  pages = {1371-1384},
  number = {8},
  abstract = {Iron-nitrogen martensite was prepared by the gaseous nitrogenization
	of iron in the austenite-phase field, followed by quenching. Low-
	and high-nitrogen contents (lath vs plate morphology) were investigated.
	Optical microscopy, transmission electron microscopy and diffraction,
	and differential thermal analysis were employed to study the crystallography
	and the tempering behavior of the martensites. The orientation relation
	between austenite and martensite, the habit plane and types of (post-)
	transformation twins were determined. Tempering-induced changes in
	hardness and release of heat were related to structural changes as
	revealed by electron diffraction. Differences with analogous iron-carbon
	martensites were discussed. Aging at room temperature leads to nitrogen
	diffusion- controlled precipitation of coherent ~"-Fel6N2 platelets,
	which process is completed after about one day. Aging at higher temperatures
	(up to about 475 K) results in incoherent a" particles, which, at
	temperatures above 460 K, exhibit small deviations (= t8 deg) from
	the usual {001}~,j~ habit plane.},
  comment = {cited in 2016MATSCIA1},
  doi = {10.1007/BF02658670},
  file = {Article:references/1985_Van_Gent_Metallurgy_Martensite.pdf:PDF},
  issn = {0360-2133},
  keywords = {metallurgy, martensite, nitrogen, crystallography},
  language = {English},
  publisher = {Springer-Verlag},
  review = {V. CONCLUSIONS A. Crystallography 1. Small orientation differences
	are usual between adjacent laths in a packet. Large orientation differences
	also oc- cur. In the latter case adjacent laths have a common (011)4,
	axis. 2. The habit plane of lath martensite is {232}r. 3. The Nishiyama-Wassermann
	orientation relationship holds approximately for austenite and high-nitrogen
	plate martensite. 4. {ll2}~,-transformation twins and {011}~,- and
	{011}~ post-transformation twins occur in plate martensite. 5. The
	habit plane of high-nitrogen plate martensite is {259}r, in agreement
	with theoretical prediction.
	
	B. Tempering 1. Aging at room temperature leads to precipitation of
	coherent a"-Fe16N2 platelets. This is accompanied by an increase
	in hardness and a release of heat. No evidence for clustering as
	a preprecipitation stage for a"-nitride was obtained. 2. Precipitation
	of a"-nitride at room temperature is nearly completed after one day
	of aging. Kinetic analysis shows that the precipitation process involves
	nitrogen diffusion- controlled growth of homogeneously nucleated
	platelets. 3. Aging at 403 K and 463 K leads to precipitation of
	incoherent ~"-FeI6N2 platelets. This is accompanied by a decrease
	in hardness and a release of heat. 4. Only one variant of the {001},,
	habit plane is used for a"-precipitation. 5. Aging at 463 K leads
	to a"-platelets each of which can be partly parallel to a habit plane
	deviating about 18 deg from {001}5,. This can be due to a misfit-stress
	effect dependent on the aspect ratio of the platelet.}
}

